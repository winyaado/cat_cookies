{
  "name": "cat_cookies",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "cat eat cookies",
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
        "service_worker": "background.js"
  },
  "host_permissions": [
    "*://*/"
  ],
  "permissions": [
    "cookies"
  ]
}